#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tuoyi/Projects/Startup/today_meal_mvp/node_modules/.pnpm/rolldown-vite@7.3.1_esbuild@0.27.2_terser@5.46.0/node_modules/rolldown-vite/bin/node_modules:/Users/tuoyi/Projects/Startup/today_meal_mvp/node_modules/.pnpm/rolldown-vite@7.3.1_esbuild@0.27.2_terser@5.46.0/node_modules/rolldown-vite/node_modules:/Users/tuoyi/Projects/Startup/today_meal_mvp/node_modules/.pnpm/rolldown-vite@7.3.1_esbuild@0.27.2_terser@5.46.0/node_modules:/Users/tuoyi/Projects/Startup/today_meal_mvp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tuoyi/Projects/Startup/today_meal_mvp/node_modules/.pnpm/rolldown-vite@7.3.1_esbuild@0.27.2_terser@5.46.0/node_modules/rolldown-vite/bin/node_modules:/Users/tuoyi/Projects/Startup/today_meal_mvp/node_modules/.pnpm/rolldown-vite@7.3.1_esbuild@0.27.2_terser@5.46.0/node_modules/rolldown-vite/node_modules:/Users/tuoyi/Projects/Startup/today_meal_mvp/node_modules/.pnpm/rolldown-vite@7.3.1_esbuild@0.27.2_terser@5.46.0/node_modules:/Users/tuoyi/Projects/Startup/today_meal_mvp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rolldown-vite@7.3.1_esbuild@0.27.2_terser@5.46.0/node_modules/rolldown-vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../rolldown-vite@7.3.1_esbuild@0.27.2_terser@5.46.0/node_modules/rolldown-vite/bin/vite.js" "$@"
fi
